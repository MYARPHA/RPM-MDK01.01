<Window x:Class="LabWork21.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabWork21"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBox x:Name="GameTextBox"/>
        <Button Content="Выбрать изображение" x:Name="SelectButton" Click="SelectButton_Click"/>
        <TextBox x:Name="LogoFilePathTextBox" IsReadOnly="True"/>
        <Button Content="Сохранить" x:Name="SaveButton" Click="SaveButton_Click"/>
    </StackPanel>
</Window>
